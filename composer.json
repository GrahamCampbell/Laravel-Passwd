{
    "name": "graham-campbell/passwd",
    "description": "Passwd Is A Simple Password Generator For Laravel 4.1",
    "keywords": ["laravel", "framework", "password generator", "password", "generator", "Passwd", "Laravel Passwd", "Laravel-Passwd", "Graham Campbell", "GrahamCampbell"],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@mineuk.com"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "sebastianbergmann/phpcov",
                "version": "1.1.0",
                "dist": {
                    "url": "https://github.com/sebastianbergmann/phpcov/archive/1.1.0.zip",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/sebastianbergmann/phpcov.git",
                    "type": "git",
                    "reference": "1.1.0"
                },
                "bin": [
                    "phpcov.php"
                ]
            }
        }
    ],
    "require": {
        "php": ">=5.4.7",
        "laravel/framework": "4.1.*",
        "symfony/config": "2.4.*",
        "symfony/filesystem": "2.4.*"
    },
    "require-dev": {
        "graham-campbell/testbench": "0.2.*"
    },
    "autoload": {
        "psr-0": {
            "GrahamCampbell\\Passwd": "src/",
            "GrahamCampbell\\Tests\\Passwd": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.4-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
